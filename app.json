{
  "expo": {
    "name": "ExTra",
    "slug": "ExpenseTracker",
    "version": "1.0.0",
    "privacy": "public",
    "description": "ExTra is an application which helps you to track your daily incomes and expenses.",
    "platforms": ["android", "ios"],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/ExTra.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.coderspace.ExTra"
    },
    "web": {
      "favicon": "./assets/icon.png"
    }
  }
}
